{
  "printWidth": 80,
  "bracketSpacing": true,
  "tabWidth": 2,
  "endOfLine": "auto",
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": true,
  "overrides": [
    {
      "files": "*.css",
      "options": {
        "singleQuote": false
      }
    }
  ],
  "plugins": ["prettier-plugin-tailwindcss"]
}
